### Replication for "Technology and Collective Action"# use "ReplicationCellPhones.dta"%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Normal Models%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%xtset coweststo: logit conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00,r cluster(cow)eststo: logit conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07,reststo: relogit conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07eststo: xtreg conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07,fe r cluster(cow)esttab using example1.tex,se ar2 scalars(F) label title() mtitles("Logit, robust SE" "Logit, robust SE" "Re-Logit" "OLS-FE, clustered SE") star(+ 0.10 * 0.05 ** 0.01 *** 0.001)eststo clear%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% IV Models%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%ivreg2 conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), rivreg2 conf2008_dum splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), rivreg2 conf2008_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), r cluster(cow)ivreg2 conf2008_dum splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), r cluster(cow)biprobit (conf2008_dum= pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07) (cell_dum_07=reg pre2000_count bdist1 capdist pop2005 mnt irri gcppc00), rbiprobit (conf2008_dum= splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07) (cell_dum_07=reg splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00), rbiprobit (conf2008_dum= pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07) (cell_dum_07=reg pre2000_count bdist1 capdist pop2005 mnt irri gcppc00), r cluster(cow)biprobit (conf2008_dum= splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 cell_dum_07) (cell_dum_07=reg splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00), r cluster(cow)ivreg2 conf2008_count pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), rivreg2 conf2008_count splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), rivreg2 conf2008_count pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), r cluster(cow)ivreg2 conf2008_count splag07_dum pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 (cell_dum_07=reg), r cluster(cow)%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Panel Models%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% use "ReplicationCellPhonesPanel.dta"xtset gwnoeststo:xtreg conf_dum cell pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 dum08 dum09,fe cluster(gwno)eststo:xtreg conf_count cell pre2000_count bdist1 capdist pop2005 mnt irri gcppc00 dum08 dum09,fe cluster(gwno)esttab using example1.tex,se ar2 scalars(F) label title() mtitles("OLS, clustered SE" "OLS, clustered SE") star(+ 0.10 * 0.05 ** 0.01 *** 0.001)eststo clearxtset gideststo:xtreg conf_dum cell dum08 dum09,fe cluster(gid)eststo:xtreg conf_count cell dum08 dum09,fe cluster(gid)esttab using example1.tex,se ar2 scalars(F) label title() mtitles("OLS, clustered SE" "OLS, clustered SE") star(+ 0.10 * 0.05 ** 0.01 *** 0.001)eststo cleareststo:xtreg lconfcount cell dum08 dum09,fe cluster(gid)eststo:xtpoisson conf_count cell dum08 dum09,feesttab using example1.tex,se ar2 scalars(F) label title() mtitles("OLS, clustered SE" "OLS, clustered SE") star(+ 0.10 * 0.05 ** 0.01 *** 0.001)eststo clear
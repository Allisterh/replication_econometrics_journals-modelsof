*Eck & Hultman (2007) "One-Sided Violence Against Civilians in War: Insights from New Fatality Data", JPR 44 (2)*Use repdata_eck&hultman.dtagen os_dummy = os_bestreplace os_dummy = 1 if os_dummy >= 1gen prior_os_dum = prior_osreplace prior_os_dum = 1 if prior_os_dum >= 1recode polity2alt (7/max=1) (nonmissing=0), gen (demo)recode polity2alt (min/0=1) (nonmissing=0), gen (auto)gen polity_sq = polity2alt*polity2alt*MODEL 1logit os_dummy war15yrs auto trade_gdp_alt prior_os_dum, cluster (cow)*MODEL 2logit os_dummy intensity_dyad auto demo govt prior_os_dum, cluster (cow)*MODEL 3nbreg os_best war15yrs auto trade_gdp_alt prior_os if os_best!=500000, nolog cluster (cow)listcoef war15yrs auto trade_gdp_alt, help percent*MODEL 4nbreg os_best intensity_dyad auto demo govt prior_os if os_best!=500000, nolog cluster (cow)listcoef intensity_dyad auto demo govt, help percent*Alternativenbreg os_best intensity_dyad polity2alt polity_sq govt prior_os if os_best!=500000, nolog cluster (cow)*MODEL 5nbreg os_best intensity_dyad auto demo prior_os if govt==1 & os_best!=500000, nolog cluster (cow)listcoef intensity_dyad auto demo, help percent*Alternativenbreg os_best intensity_dyad polity2alt polity_sq prior_os if govt==1 & os_best!=500000, nolog cluster (cow)*MODEL 6nbreg os_best intensity_dyad auto demo prior_os if govt==0, nolog cluster (cow)listcoef intensity_dyad auto demo, help percent*Alternativenbreg os_best intensity_dyad polity2alt polity_sq prior_os if govt==0, nolog cluster (cow)